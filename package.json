{
  "name": "softonix-guidelines-vite",
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev-only": "vite --host",
    "dev": "run-s dev-only",
    "build-only": "vite build",
    "build": "run-s type-check build-only",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "openapi-generate": "npx openapi-typescript https://fakerestapi.azurewebsites.net/swagger/v1/swagger.json -o ./src/api/types/schema.d.ts",
    "postinstall": "run-p openapi-generate"
  },
  "dependencies": {
    "@vueuse/core": "^12.3.0",
    "axios": "^1.7.9",
    "element-plus": "^2.9.2",
    "js-confetti": "^0.13.1",
    "pinia": "^2.3.0",
    "portal-vue": "^3.0.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@softonix/eslint-config-vue": "^0.1.2",
    "@softonix/vite-plugin-auto-imports-vue": "^0.1.3",
    "@tailwindcss/vite": "^4.1.12",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.17.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "pathe": "^2.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
